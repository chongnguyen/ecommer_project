                    span Qu&#x1EA3;n l&yacute; &dstrok;&#x1A1;n h&agrave;ng
                    i.fas.fa-angle-down
                  #sidebarDropdown.sidebar__dropdown
                    a.sidebar__dropdown-item(href='/seller/bill') T&#x1EA5;t c&#x1EA3;
                    a.sidebar__dropdown-item(href='/seller/bill?type=0') Ch&#x1EDD; x&aacute;c nh&#x1EAD;n
                    a.sidebar__dropdown-item(href='/seller/bill?type=1') Đang giao
                    a.sidebar__dropdown-item(href='/seller/bill?type=2') Đã giao
                    a.sidebar__dropdown-item(href='/seller/bill?type=3') Đã hủy
                .sidebar__item.active
                  p.sidebar__heading
                    img(src='https://cf.shopee.vn/file/6b1ffcde1ff12621088110f419a5283a' alt='')
                    span Qu&#x1EA3;n l&yacute; shop
                    i.fas.fa-angle-down
                  #sidebarDropdown.sidebar__dropdown
                    a.sidebar__dropdown-item(href='/seller/profile') H&#x1ED3; s&#x1A1; shop
            .col.l-10
              block content
                main.main
                  .todo
                    h3.todo__heading Nh&#x1EEF;ng vi&#x1EC7;c c&#x1EA7;n l&agrave;m
                    p.todo__heading-sub Nh&#x1EEF;ng vi&#x1EC7;c b&#x1EA1;n s&#x1EBD; ph&#x1EA3;i l&agrave;m
                    .todo__content
                      .row
                        -n = 0;
                        each tab, i in tabs
                          .col.l-3
                            a.todo__item.separate(href='/seller/bill?type=' + i)
                              p.todo__item-num= stateBill[n++] || 0
                              p.todo__item-label= tab
                        .col.l-3
                          a.todo__item.separate.btn-alert(href='javascript:function() { return false; }')
                            p.todo__item-num= stateBill[n++] || 0
                            p.todo__item-label Ch&#x1B0;&#x1A1;ng tr&igrave;nh khuy&#x1EBF;n m&atilde;i
                        .col.l-3
                          a.todo__item.separate.btn-alert(href='javascript:function() { return false; }')
                            p.todo__item-num= stateBill[n++] || 0
                            p.todo__item-label Tr&#x1EA3; h&agrave;ng / Ho&agrave;n ti&#x1EC1;n
                        .col.l-3
                          a.todo__item.separate.btn-alert(href='javascript:function() { return false; }')
                            p.todo__item-num= stateBill[n++] || 0
                            p.todo__item-label S&#x1EA3;n ph&#x1EA9;m h&#x1EBF;t h&agrave;ng
                        .col.l-3
                          a.todo__item.btn-alert(href='javascript:function() { return false; }')
                            p.todo__item-num= stateBill[n++] || 0
